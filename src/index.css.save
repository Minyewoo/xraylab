@import url('https://fonts.googleapis.com/css?family=Montserrat:400,700&display=swap&subset=cyrillic');
html {
  min-height: 100%;
}

body {
  margin: 0;
  padding: 0;
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen",
    "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue",
    sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  background-color: #eeeeee;
  min-height: 100%;
}

form {
  padding: 30px;
  margin-top: 60px;
}

form button, form h5 {
  margin: 20px 0;
}

input[type=text]:not(.browser-default):focus:not([readounly]),
input[type=email]:not(.browser-default):focus:not([readounly]),
input[type=password]:not(.browser-default):focus:not([readounly]),
textarea.materialize-textarea:focus:not([readonly]) {
  border-color: #ec407a;
  box-shadow: none;
  
}

code {
  font-family: source-code-pro, Menlo, Monaco, Consolas, "Courier New",
    monospace;
}

.t-btn {
  background: #151515;
  border: none;
  border-radius: 54px;
  
  padding: 12px 23px;
}

.t-btn:hover {
  background: #303030;
}

.t-btn:active {
  background: #404040;
}

.up-txt {
  font-family: Montserrat;
  font-style: normal;
  font-weight: bold;
  font-size: 18px;
  line-height: 22px;
  
  text-align: center;
  text-transform: uppercase;
  
  color: #ffffff;
}

.row .col {
  padding: 0 1.5rem;

}

/* Top Navbar */

.top-navbar {
  position: fixed;
  top: 0;
  display: -webkit-flex; 
  -webkit-justify-content: space-between;
  -webkit-align-items: center;
  display: flex;
  justify-content: space-between;
  align-items: center;

  background-color: #ffffff;
  width: 100%;
  height: 93px;
  
  margin: 0 0 32px 0;
  padding: 0 64px;
  z-index: 1;
  line-height: 93px;
}





.navbar-buttons li a{
  color: black;
}

.logo {
  font-family: Montserrat;
  font-style: normal;
  font-weight: 600;
  font-size: 24px;
  line-height: 29px;
  
  color: #151515;
}

.App {
  margin: 0;
  padding: 0;
}

.dashboard {
  margin-top: 100px;
}

.container {
  margin-top: 100px;
}
